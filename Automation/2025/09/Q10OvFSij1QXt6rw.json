    {
      "parameters": {
        "jsCode": "// Obtener la cantidad de requests del input\nconst cantidadRequest = $input.first().json['cantidad de request'];\n\n// Validar que sea un número válido\nif (!cantidadRequest || cantidadRequest <= 0 || !Number.isInteger(cantidadRequest)) {\n  return [{\n    json: { error: 'La cantidad de request debe ser un número entero mayor a 0' }\n  }];\n}\n\n// Crear un array con la cantidad de objetos solicitada\nconst items = [];\n\nfor (let i = 0; i < cantidadRequest; i++) {\n  items.push({\n    json: {\n      index: i + 1, // Índice comenzando en 1\n      total: cantidadRequest,\n      requestNumber: i + 1\n    }\n  });\n}\n\n// Retornar el array de objetos para el Loop Over Items\nreturn items;\n\n"
      },

        1056,
        -80
      ],
      "id": "3e06df02-2ef4-469a-89e9-db0427eab51f",
      "name": "clientes3",
      "alwaysOutputData": true,
      "credentials": {
        "nocoDbApiToken": {
          "id": "PngKKQ0D2nKSCDu8",
          "name": "Tool Acondicionados SAS"
        }
      },
      "onError": "continueRegularOutput"
    },
    {
      "parameters": {
        "jsCode": "// 1) Palabra que viene del Start\nconst termino = $('Start').first().json.razon_social || \"\";\n\n// 2) Lista de clientes que llega como input a este nodo\nconst clientes = items.map(i => i.json);\n\n// 3) Normalizar para comparación sin tildes ni mayúsculas\nfunction norm(str) {\n  return (str || \"\")\n    .toString()\n    .normalize(\"NFD\")\n    .replace(/[\\u0300-\\u036f]/g, \"\")\n    .toLowerCase()\n    .trim();\n}\n\nconst buscar = norm(termino);\n\n// 4) Si no hay término, no devolvemos nada\nif (!buscar) {\n  return []; // el Switch siguiente manejará esto\n}\n\n// 5) Buscar coincidencia exacta o muy cercana\nlet mejor = null;\nlet mejorDistancia = Infinity;\n\nfunction distancia(a, b) {\n  a = norm(a); b = norm(b);\n  if (a === b) return 0;\n  if (!a || !b) return Infinity;\n  \n  const dp = Array(b.length + 1).fill(null).map(() =>\n    Array(a.length + 1).fill(0)\n  );\n  for (let i = 0; i <= b.length; i++) dp[i][0] = i;\n  for (let j = 0; j <= a.length; j++) dp[0][j] = j;\n\n  for (let i = 1; i <= b.length; i++) {\n    for (let j = 1; j <= a.length; j++) {\n      const cost = b[j - 1] === a[i - 1] ? 0 : 1;\n      dp[i][j] = Math.min(\n        dp[i-1][j] + 1,\n        dp[i][j-1] + 1,\n        dp[i-1][j-1] + cost\n      );\n    }\n  }\n  return dp[b.length][a.length];\n}\n\nfor (const c of clientes) {\n  const nombre = c.razon_social || c.nombre || \"\";\n  const d = distancia(buscar, nombre);\n  if (d < mejorDistancia) {\n    mejorDistancia = d;\n    mejor = c;\n  }\n}\n\n// 6) Umbral de tolerancia (ajusta si deseas)\nif (mejorDistancia <= 2 && mejor) {\n  // ✅ Devolvemos UN SOLO ITEM\n  return [{ json: mejor }];\n}\n\n// 7) ❗ Si no encontró nada, devolvemos vacío\nreturn [];\n"
      },
